{
	"name": "montavistamun",
	"description": "Monta Vista Model United Nations Club Website",
	"version": "2.0.0",
	"author": "Jeffrey Meng <jeffkmeng@gmail.com>",
	"dependencies": {
		"@tailwindcss/custom-forms": "0.2.1",
		"@tailwindcss/ui": "^0.3.0",
		"@types/react": "^16.9.38",
		"@types/react-helmet": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^3.3.0",
		"@typescript-eslint/parser": "^3.3.0",
		"autoprefixer": "9.8.2",
		"babel-eslint": "10.1.0",
		"cssnano": "4.1.10",
		"eslint": "7.3.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-import": "2.21.2",
		"eslint-plugin-react": "7.20.0",
		"gatsby": "2.23.7",
		"gatsby-plugin-eslint": "2.0.8",
		"gatsby-plugin-manifest": "2.4.12",
		"gatsby-plugin-offline": "3.2.11",
		"gatsby-plugin-postcss": "^2.3.4",
		"gatsby-plugin-react-helmet": "3.3.4",
		"gatsby-plugin-sharp": "2.6.12",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"postcss-import": "^12.0.1",
		"prettier": "^2.0.5",
		"prop-types": "15.7.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-helmet": "6.1.0",
		"tailwindcss": "1.4.6",
		"typeface-inter": "^3.12.0",
		"typescript": "^3.9.5"
	},
	"keywords": [
		"gatsby",
		"tailwind",
		"tailwindcss",
		"purgecss"
	],
	"license": "MIT",
	"scripts": {
		"analyze:lint": "eslint --ext .jsx --ext .js .",
		"analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
		"analyze": "npm run analyze:lint && npm run analyze:prettier",
		"fix:lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx . --fix",
		"fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
		"fix": "npm run fix:lint && npm run fix:prettier",
		"build": "gatsby build",
		"develop": "gatsby develop",
		"dev": "npm run develop",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,jsx,tsx}": "eslint --cache",
		"*.{ts,tsx,js,jsx,json,md,scss,css}": "prettier --write"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jeffreymeng/montavistamun"
	},
	"bugs": {
		"url": "https://github.com/jeffreymeng/montavistamun/issues"
	}
}
